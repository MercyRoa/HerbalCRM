<h1>Listing text_models</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @text_models.each do |text_model| %>
  <tr>
    <td><%= text_model.title %></td>
    <td><%= text_model.body %></td>
    <td><%= link_to 'Show', text_model %></td>
    <td><%= link_to 'Edit', edit_text_model_path(text_model) %></td>
    <td><%= link_to 'Destroy', text_model, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

