<h1>Listing messages</h1>

<table>
  <tr>
    <th>Account</th>
    <th>Lead</th>
    <th>From</th>
    <th>To</th>
    <th>Headers</th>
    <th>Body</th>
    <th>Body raw</th>
    <th>Readed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @messages.each do |message| %>
  <tr>
    <td><%= message.account_id %></td>
    <td><%= message.lead_id %></td>
    <td><%= message.from %></td>
    <td><%= message.to %></td>
    <td><%= message.headers %></td>
    <td><%= message.body %></td>
    <td><%= message.body_raw %></td>
    <td><%= message.readed %></td>
    <td><%= link_to 'Show', message %></td>
    <td><%= link_to 'Edit', edit_message_path(message) %></td>
    <td><%= link_to 'Destroy', message, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Message', new_message_path %>
