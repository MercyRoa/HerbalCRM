<h1>Listing lead_details</h1>

<table>
  <tr>
    <th>Lead</th>
    <th>Key</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lead_details.each do |lead_detail| %>
  <tr>
    <td><%= lead_detail.lead_id %></td>
    <td><%= lead_detail.key %></td>
    <td><%= lead_detail.value %></td>
    <td><%= link_to 'Show', lead_detail %></td>
    <td><%= link_to 'Edit', edit_lead_detail_path(lead_detail) %></td>
    <td><%= link_to 'Destroy', lead_detail, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

